<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kiwi.auready_ver2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".taskheads.TaskHeadsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".friend.FriendsActivity" />
        <activity android:name=".friend.FindActivity" />
        <activity
            android:name=".tasks.TasksActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".taskheaddetail.TaskHeadDetailActivity"></activity>
    </application>

</manifest>